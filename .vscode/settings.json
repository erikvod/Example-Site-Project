  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "files.autoSave": "onFocusChange",
  "todohighlight.isCaseSensitive": false,
  "todohighlight.include": [
    "*",
    "ts"
  ],
  "todohighlight.keywords": [
    " todo"
  ],
  "todohighlight.keywordsPattern": "\\stodo\\s.*",
}